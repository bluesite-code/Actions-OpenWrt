# .github/workflows/your-workflow-name.yml
name: Workflow-1

on:
  push:
    branches: [ main ]
  pull_request:
    branches: [ main ]

jobs:
  build:
    name: Build
    runs-on: ubuntu-latest
    steps:
      - name: Checkout Code
        uses: actions/checkout@v2
      - name: Set up Go 1.23
        uses: actions/setup-go@v2
        with:
          go-version: 1.23
      - name: Install Dependencies
        run: go mod download
      - name: Build Project
        run: go build -v ./...
